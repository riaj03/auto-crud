{
  "name": "rest",
  "version": "1.0.0",
  "description": "Restfull API generstor with node.js and express",
  "main": "./src/index.ts",
  "bin": {
    "rest": "dist/index.js"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "prestart": "npm run tsc",
    "app": "node dist/index.js",
    "start": "node .",
    "start:dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "cd ./src/db && sequelize db:migrate",
    "migrate:undo": "cd ./src/db && sequelize db:migrate:undo",
    "format": "prettier --config .prettierrc.json --write src/**/*.{js,ts}",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix"
  },
  "keywords": [
    "REST",
    "generate",
    "rest",
    "rest",
    "api",
    "node",
    "rest",
    "express",
    "rest",
    "autometic",
    "api",
    "generator",
    "by",
    "express"
  ],
  "author": "riaj.csp@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "prettier": "^2.6.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/pluralize": "0.0.29",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "js-beautify": "^1.13.0",
    "mysql2": "^2.2.5",
    "pluralize": "^8.0.0",
    "rest": "^2.0.0",
    "sequelize": "^6.3.5"
  }
}
